{
  "name": "@nitm/contracting-marketing-template",
  "version": "2.0.12",
  "description": "a Nuxt 3 template and boilerplate with a lot of useful features. Nuxt 3 + Tailwindcss + Nuxt Layer",
  "author": "ninjasitm",
  "private": false,
  "license": "MIT",
  "type": "module",
  "main": "./nuxt.config.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/ninjasitm/contracting-marketing-template/git"
  },
  "files": [
    "assets",
    "components",
    "composables",
    "layouts",
    "modules",
    "pages",
    "public",
    "stores",
    "app.config.ts",
    "app.vue",
    "nuxt.config.ts",
    "nuxt.schema.ts",
    "tailwind.config.ts",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "nuxt build ./app",
    "dev": "nuxt dev ./app",
    "generate": "nuxt generate ./app",
    "preview": "pnpm build && nuxt preview ./app --port 3000",
    "postinstall": "nuxt prepare ./app",
    "dev:starter": "nuxt dev ./examples/starter",
    "lint": "eslint --fix --ext \".ts,.js,.vue\" . && vue-tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "release": "release-it"
  },
  "dependencies": {
    "@jagaad/vue-hubspot-form": "^2.2.3",
    "@nuxt/content": "^2.13.4",
    "@nuxt/devtools": "^1.7.0",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/image": "^1.10.0",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/svg": "^0.4.1",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@pinia/nuxt": "^0.5.5",
    "@vueuse/core": "^10.11.1",
    "@vueuse/nuxt": "^10.11.1",
    "moment": "^2.30.1",
    "nuxt-headlessui": "^1.2.0",
    "nuxt-icon": "^0.6.10",
    "nuxt-svgo-loader": "^0.4.1",
    "pinia": "^2.3.1",
    "sass": "^1.89.0",
    "stringify": "^2.2.0",
    "tailwindcss": "^3.4.17",
    "unplugin-vue-components": "^0.25.2",
    "worker-mailer": "^1.1.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@nuxt/config": "^2.18.1",
    "@nuxt/eslint": "1.4.1",
    "@nuxt/kit": "^3.17.4",
    "@nuxt/test-utils": "^3.19.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^1.6.1",
    "@vue/test-utils": "^2.4.6",
    "consola": "^3.4.2",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-es-x": "^8.6.2",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-vue": "^10.1.0",
    "jsdom": "^26.1.0",
    "nuxt": "^3.17.4",
    "release-it": "^16.3.0",
    "serve": "^14.2.4",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.3.5",
    "vitest": "^1.6.1",
    "vue": "^3.5.14",
    "vue-tsc": "^2.2.10",
    "webpack": "^5.99.9"
  },
  "release-it": {
    "github": {
      "release": true,
      "releaseName": "v${version}"
    },
    "git": {
      "commitMessage": "chore(release): release v${version}"
    }
  },
  "packageManager": "pnpm@8.5.1+sha1.97019f5a8ec2416123506419ab36dd558e4c72eb"
}
